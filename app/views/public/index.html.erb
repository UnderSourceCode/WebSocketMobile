<div>
  <h1>Chat for PC</h1>
</div>
<div id="chat" class="well" data-uri="<%= request.host %>:<%= request.port %>/websocket">
</div>
<div class="form-inline well">
  <fieldset>
    <input type="text" class="input-medium" id="user_name" placeholder="name">
    <input type="text" class="input-xlarge" id="message" placeholder="message">
    <button id="send" class="btn btn-primary">Send</button>
  </fieldset>
</div>